{
  "projects": {
    "homepage": {
      "root": "homepage",
      "builds": [
        {
          "src": "homepage/homepage/package.json",
          "use": "@vercel/node"
        }
      ],
      "routes": [
        {
          "src": "/(.*)",
          "dest": "homepage/homepage/$1"
        }
      ]
    }
  }
}
