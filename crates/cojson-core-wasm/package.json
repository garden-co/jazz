{
  "name": "cojson-core-wasm",
  "type": "module",
  "version": "0.18.15",
  "files": [
    "public/cojson_core_wasm.js",
    "public/cojson_core_wasm.d.ts",
    "public/cojson_core_wasm.wasm.js",
    "public/cojson_core_wasm.wasm.d.ts",
    "public/cojson_core_wasm.wasm",
    "index.js",
    "index.d.ts"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "pnpm run build:wasm",
    "build:wasm": "wasm-pack build --release --target web && node build.js",
    "build:dev": "wasm-pack build --dev --target web && node build.js"
  },
  "exports": {
    ".": {
      "default": "./index.js",
      "types": "./index.d.ts"
    },
    "./edge-lite": {
      "default": "./edge-lite.js",
      "types": "./index.d.ts"
    }
  },
  "devDependencies": {
    "wasm-pack": "^0.13.1"
  }
}
