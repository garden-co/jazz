    "svelte": "catalog:svelte",
    "svelte-check": "^4.0.0",
    "svelte-sonner": "^1.0.7",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
