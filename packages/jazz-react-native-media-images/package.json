{
  "name": "jazz-react-native-media-images",
  "version": "0.8.17",
  "type": "module",
  "main": "dist/index.js",
  "types": "src/index.ts",
  "license": "MIT",
  "dependencies": {
    "jazz-tools": "workspace:*"
  },
  "devDependencies": {
    "@bam.tech/react-native-image-resizer": "^3.0.10",
    "expo-file-system": "^17.0.1",
    "react-native": "~0.74.5",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@bam.tech/react-native-image-resizer": "*",
    "expo-file-system": "*",
    "react-native": "*"
  },
  "scripts": {
    "dev": "tsup src/index.ts --format esm --watch",
    "test": "vitest --run --root ../../ --project jazz-react-native-media-images",
    "test:watch": "vitest --watch --root ../../ --project jazz-react-native-media-images",
    "format-and-lint": "biome check .",
    "format-and-lint:fix": "biome check . --write",
    "typecheck": "tsc --noEmit --sourceMap -p tsconfig.json",
    "build": "rm -rf ./dist && tsup src/index.ts --format esm",
    "prepublishOnly": "npm run build"
  }
}
