{
  "name": "jazz-tools",
  "module": "dist/index.web.js",
  "main": "dist/index.web.js",
  "types": "src/index.web.ts",
  "react-native": "dist/index.native.js",
  "exports": {
    ".": {
      "react-native": "./dist/ndex.native.js",
      "types": "./src/index.web.ts",
      "default": "./dist/index.web.js"
    },
    "./native": {
      "react-native": "./dist/index.native.js",
      "types": "./src/index.native.ts",
      "default": "./dist/index.native.js"
    },
    "./src/*": "./src/*"
  },
  "type": "module",
  "license": "MIT",
  "version": "0.8.21",
  "dependencies": {
    "cojson": "workspace:*",
    "fast-check": "^3.17.2"
  },
  "scripts": {
    "format-and-lint": "biome check .",
    "format-and-lint:fix": "biome check . --write",
    "dev": "pnpm tsup src/index.native.ts src/index.web.ts --format esm --watch",
    "test": "vitest --run --root ../../ --project jazz-tools",
    "test:watch": "vitest --watch --root ../../ --project jazz-tools",
    "typecheck:web": "tsc --noEmit --sourceMap -p tsconfig.web.json",
    "typecheck:native": "tsc --noEmit --sourceMap -p tsconfig.native.json",
    "typecheck": "pnpm typecheck:web && pnpm typecheck:native",
    "build": "rm -rf ./dist && pnpm tsup src/index.native.ts src/index.web.ts --format esm",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "vitest": "1.5.3"
  },
  "madge": {
    "detectiveOptions": {
      "ts": {
        "skipTypeImports": true
      }
    }
  }
}
