{
  "name": "jazz-react-auth-clerk",
  "version": "0.8.21",
  "type": "module",
  "main": "dist/index.js",
  "types": "src/index.tsx",
  "license": "MIT",
  "dependencies": {
    "cojson": "workspace:0.8.21",
    "jazz-browser-auth-clerk": "workspace:0.8.21",
