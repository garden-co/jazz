{
  "name": "jazz-react-prosemirror",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "dependencies": {
    "jazz-browser-prosemirror": "workspace:*",
    "jazz-react": "workspace:*",
    "jazz-tools": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "scripts": {
    "format-and-lint": "biome check .",
    "format-and-lint:fix": "biome check . --write",
    "build": "rm -rf ./dist && tsc --sourceMap --declarationMap --declaration --outDir dist",
    "prepublishOnly": "npm run build",
    "test": "vitest --run --root ../../ --project jazz-react-prosemirror",
    "test:watch": "vitest --watch --root ../../ --project jazz-react-prosemirror"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.3.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.2.1",
    "typescript": "^5.0.2",
    "vitest": "1.5.3"
  }
}
